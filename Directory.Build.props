<Project>
    <PropertyGroup>
        <Authors>Geekiam</Authors>
        <TargetFramework>net8.0</TargetFramework>
        <Title>Geekiam Web Scraping Utility</Title>
        <Description>Geekiam</Description>
        <RepositoryType>git</RepositoryType>
        <PackageTags>rss, dotnet, feeds</PackageTags>
        <RepositoryUrl>https://github.com/geekiam/web-scraping</RepositoryUrl>
        <Copyright>Copyright Â© $([System.DateTime]::Now.ToString("yyyy")) Geekiam</Copyright>
        <PackageProjectUrl>https://geekiam.io</PackageProjectUrl>
        <license>https://github.com/geekiam/web-scraping/blob/master/LICENSE</license>
        <PackageIcon>icon.png</PackageIcon>
        <Version></Version>
        <RootNamespace>Geekiam</RootNamespace>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
        <Content Include="$(MSBuildThisFileDirectory)icon.png" Pack="true" PackagePath="/">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </Content>
    </ItemGroup>
</Project>